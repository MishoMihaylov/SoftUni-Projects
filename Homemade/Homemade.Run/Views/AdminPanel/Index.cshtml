@{
    ViewBag.Title = "Admin Panel";
}

<div class="form">
    <div class="container">
        <div class="row">
            <div class="col-md-2" id="header"><p class="text-center"><span class="glyphicon glyphicon-user"></span></p></span></div>
            <div class="col-sm-10" id="header2"><p class="text-center register">Admin Panel</p></div>
        </div>
        <div class="row">
            <div>
                <h6 class="text-center">Your personal details</h6>
            </div>
            <hr>
            <div class="form-group">
                <div class="row">
                    <div class="col-md-5 col-md-offset-1"><label for="name">Name:</label></div>
                    <div class="col-md-5 "><label for="lastname"> Last Name:</label></div>
                </div>

                <div class="row">
                    <div class="col-md-5 col-md-offset-1"> <input type="name" class="form-control" id="name"></div>
                    <div class="col-md-5 "> <input type="lastname" class="form-control" id="lastname"></div>
                </div>
            </div>
       </div>
    </div>
</div>
